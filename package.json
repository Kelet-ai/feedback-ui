{
  "name": "@kelet-ai/feedback-ui",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelet-ai/feedback-ui.git"
  },
  "main": "./dist/feedback-ui.umd.js",
  "module": "./dist/feedback-ui.es.js",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-themes": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/builder-vite": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.1.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.525.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "shadcn": "^2.10.0",
    "storybook": "^9.1.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@opentelemetry/api": "^1.9.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": {
        "import": "./dist/feedback-ui.es.js",
        "require": "./dist/feedback-ui.umd.js"
      },
      "production": {
        "import": "./dist/feedback-ui.es.min.js",
        "require": "./dist/feedback-ui.umd.min.js"
      },
      "import": "./dist/feedback-ui.es.min.js",
      "require": "./dist/feedback-ui.umd.min.js"
    }
  },
  "description": "A headless React component library for collecting explicit and implicit user feedback, with automatic state change tracking",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "components",
    "feedback",
    "ui",
    "typescript"
  ],
  "license": "APACHE-2.0",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 storybook dev -p 6006",
    "build": "tsc -b && bun run build:prod && bun run build:dev && bun run build:merge",
    "build:dev": "vite build --mode development --outDir dist-dev",
    "build:prod": "vite build --mode production --outDir dist-prod",
    "build:merge": "mkdir -p dist && cp -r dist-prod/* dist/ && cp dist-dev/feedback-ui.*.js dist/ && cp dist-dev/feedback-ui.*.js.map dist/ && rm -rf dist-dev dist-prod",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest --project=unit",
    "test:storybook": "vitest --project=storybook",
    "test:storybook:coverage": "vitest --project=storybook --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite preview",
    "prepublishOnly": "bun run build",
    "storybook": "NODE_NO_WARNINGS=1 storybook dev -p 6006",
    "registry:build": "bun shadcn build",
    "typecheck": "tsc -b",
    "verify-build": "bun test ./.github/build-verification.test.ts",
    "checks": "bun run lint:fix && bun run prettier:fix && bun run typecheck && bun run test:unit --run && bun run test:storybook --run"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@types/deep-diff": "^1.0.5",
    "@types/diff": "^8.0.0",
    "@types/fast-levenshtein": "^0.0.4",
    "deep-diff": "^1.0.2",
    "diff": "^8.0.2",
    "fast-levenshtein": "^3.0.0"
  }
}
